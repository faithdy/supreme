<% layout('header') -%>

<div class="container">
    <div class="row">
        <div class="jumbotron">
            <div class="page-header">
                <h1><%= title %></h1>
            </div>
            <table class="table table-hover table-striped">
                <tr class="row">
                    <th class="col-md-3 col-sm-3"><h3>아이디</h3></th>
                    <th class="col-md-3 col-sm-3"><h3>이름</h3></th>
                    <th class="col-md-3 col-sm-3"><h3>학번</h3></th>
                    <th class="col-md-3 col-sm-3"><h3>이-메일</h3></th>
                </tr>
                <% if(data) { %>
                    <% for(var i = 0, len = data.length; i < len, item = data[i]; i++) { %>
                    <tr class="row">
                        <td class="col-md-3 col-sm-3"><h4><%=item.id%></h4></td>
                        <td class="col-md-3 col-sm-3"><h4><%=item.name%></h4></td>
                        <td class="col-md-3 col-sm-3"><h4><%=item.hakbun%></h4></td>
                        <td class="col-md-3 col-sm-3"><h4><%=item.email%></h4></td>
                    </tr>
                    <% } %>
                <% } %>
            </table>
            <a href="/board/list" class="btn btn-lg btn-info btn-raised">목록 <i class="material-icons">list</i></a>
        </div>
    </div>
</div>